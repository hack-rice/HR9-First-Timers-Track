{
	"manifest_version": 2, 
	"name": "My first extension!",
	"version": "0.1",
  	"browser_action": {
  		"default_title": "Sample Extension",
    	"default_icon": "icon.png",
    	"default_popup": "popup.html"
	  }, 
	"content_scripts": [
	{
		"matches": [
		"<all_urls>"
		],
		"js": ["content.js"]
	}
	],
	"background" : {
		"scripts": ["background.js"],
      	"persistent": true
	},
  	"permissions": [
		  "activeTab",
		  "storage",
		  "tabs",
		  "webNavigation"
  	]
}